<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>
    <%= render 'shared/error_messages', obj: @user %>

    <%= f.label :email %>
    <%= f.email_field :email %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
<% end %>

<p>New user? <%= link_to("Sign up", new_user_path) %></p>