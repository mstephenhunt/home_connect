

<li>
    <h2><%= plug.name %></h2> 
    <span><%= plug.state %> | 
    <% if !plug.feed_id.nil? %>
        ID: <%= plug.feed_id %> |
    <% else %>
        No feed id |
    <% end %>
    <%= link_to "Flip", flip_plug_path(plug) %> |
    <%= link_to "Delete", plug, method: :delete %> | 
    <%= link_to "Edit", edit_plug_path(plug) %>
    </span>
</li>